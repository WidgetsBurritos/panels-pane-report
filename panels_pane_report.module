<?php

/**
 * @file
 * Contains panels_pane_report.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function panels_pane_report_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the panels_pane_report module.
    case 'help.page.panels_pane_report':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Generates usage reports for panels panes.') . '</p>';
      return $output;

    default:
  }
}

